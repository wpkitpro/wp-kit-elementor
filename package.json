{
	"name": "wp-kit-elementor",
	"slug": "wp-kit-elementor",
	"homepage": "https://wpkit.pro",
	"description": "A sample WordPress theme for Elementor page builder.",
	"version": "1.0.4",
	"private": true,
	"scripts": {
		"update-version": "node .github/scripts/update-version-in-files.js",
		"start": "chokidar \"**/*.scss\" -c \"npm run build\" --initial",
		"build:style": "sass assets/sass/style.scss:style.css --style=expanded --source-map && sass assets/sass/style.scss:style.min.css --style compressed --no-source-map",
		"build:style-editor": "sass assets/sass/style-editor.scss:style-editor.css --style=expanded --source-map && sass assets/sass/style-editor.scss:style-editor.min.css --style compressed --no-source-map",
		"build:stylelint": "stylelint **/*.css --fix --config .stylelintrc-css.json",
		"build": "run-s \"build:*\"",
		"watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial",
		"lint:scss": "stylelint **/*.scss",
		"lint-fix:scss": "stylelint **/*.scss --fix",
		"bundle": "dir-archiver --src . --dest ../wp-kit-elementor.zip --exclude .DS_Store node_modules .editorconfig .eslintrc.js .gitattributes .gitignore .prettierrc .stylelintrc.json mix-manifest.json package.json style.scss style-editor.scss webpack.mix.js yarn.lock package-lock.json .git .gitattributes .gitignore README.md LICENSE"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-optional-chaining": "^7.21.0",
		"@babel/preset-env": "^7.23.7",
		"@wordpress/components": "^25.14.0",
		"@wordpress/eslint-plugin": "^17.5.0",
		"@wordpress/i18n": "^4.48.0",
		"@wordpress/notices": "^4.16.0",
		"@wordpress/scripts": "^26.19.0",
		"autoprefixer": "^10.4.16",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^9.1.3",
		"concurrently": "^8.2.2",
		"copy-webpack-plugin": "^11.0.0",
		"cssnano": "^6.0.3",
		"dir-archiver": "^2.1.0",
		"eslint": "^8.56.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-no-jquery": "^2.7.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-webpack-plugin": "^4.0.1",
		"grunt": "^1.6.1",
		"grunt-checktextdomain": "^1.0.1",
		"grunt-contrib-sass": "^2.0.0",
		"grunt-contrib-watch": "^1.1.0",
		"grunt-sass": "^3.1.0",
		"grunt-text-replace": "^0.4.0",
		"grunt-webpack": "^6.0.0",
		"grunt-wp-readme-to-markdown-with-extra": "^2.2.0",
		"load-grunt-tasks": "^5.1.0",
		"matchdep": "^2.0.0",
		"npm-build-zip": "^1.0.4",
		"react": "^18.2.0",
		"rimraf": "^5.0.5",
		"sass": "^1.69.7",
		"terser-webpack-plugin": "^5.3.10",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.23.3",
		"@babel/plugin-transform-runtime": "^7.23.7",
		"@lodder/grunt-postcss": "^3.1.1",
		"grunt-autoprefixer": "^3.0.4",
		"grunt-cssnano": "^3.0.0"
	}
}
